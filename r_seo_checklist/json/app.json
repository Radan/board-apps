        {
            "id": "561cadb7433c28c6e4768843",
            "name": "Onsite Optimization",
            "idBoard": "561c98eeb0b30d751895ab92",
            "idCard": "5666c95a9b21ec1d69a06bf3",
            "pos": 1,
            "checkItems": [
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd94",
                    "name": "Write a unique title tag for each webpage, using one or more of the target keywords you’ve assigned to that page",
                    "nameData": null,
                    "pos": 1
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd95",
                    "name": "Keep the entire title tag under 60 characters",
                    "nameData": null,
                    "pos": 2
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd96",
                    "name": "Write a unique meta description for each webpage, using one or more of the target keywords you’ve assigned to that page",
                    "nameData": null,
                    "pos": 3
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd97",
                    "name": "Keep the entire meta description under 155 characters",
                    "nameData": null,
                    "pos": 4
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd98",
                    "name": "Include a target keyword in each page’s URL",
                    "nameData": null,
                    "pos": 5
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd99",
                    "name": "Check that each page has one (and only one) h1 tag, preferably including a target keyword",
                    "nameData": null,
                    "pos": 6
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd00",
                    "name": "Additional headers (h2 – h6), if applicable, should appear in order, lower on the page",
                    "nameData": null,
                    "pos": 7
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd01",
                    "name": "Add search feature (Google Custom Search, Consider 'Open Search')",
                    "nameData": null,
                    "pos": 8
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd02",
                    "name": "Make sure all page on your website can be accessed in less than 3 clicks from the homepage.",
                    "nameData": null,
                    "pos": 9
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd03",
                    "name": "If you have pages (or whole sections) buried deeper than that, consider linking to them from your main navigation, or an applicable category page.",
                    "nameData": null,
                    "pos": 10
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd04",
                    "name": "When you link to a given webpage from elsewhere on the site, use one of the target keywords associated with the page you are linking to as (part of) the anchor text.",
                    "nameData": null,
                    "pos": 11
                },
                {
                    "state": "incomplete",
                    "id": "56b9e2586254881993d2cd05",
                    "name": "If your website is image-heavy, add ALT tags to the images to let search engines understand what visuals users are seeing.",
                    "nameData": null,
                    "pos": 12
                }
            ]
        },
        {
